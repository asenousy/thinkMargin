{
  "expo": {
    "name": "thinkMargin",
    "slug": "think-margin",
    "version": "6.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "{{AD_UNIT_ID_IOS}}"
      },
      "appStoreUrl": "https://apps.apple.com/us/app/thinkmargin/id1042241713",
      "bundleIdentifier": "com.sensoApps.thinkMargin"
    },
    "android": {
      "icon": "./assets/icon-android.png",
      "softwareKeyboardLayoutMode": "pan",
      "config": {
        "googleMobileAdsAppId": "{{AD_UNIT_ID_ANDROID}}"
      },
      "package": "com.sensoapps.asenousy.thinkmargin",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.sensoapps.asenousy.thinkmargin"
    }
  }
}
